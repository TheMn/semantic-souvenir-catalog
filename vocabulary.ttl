@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix my: <https://themn.github.io/semantic-souvenir-catalog/vocabulary.ttl#> .

# My Travel Souvenirs Vocabulary (T-Box)
# This file defines the custom terms for the project.
# It does NOT contain instance data.

# Class Definition
my:Souvenir a rdfs:Class ;
    rdfs:label "Souvenir" ;
    rdfs:comment "A memento bought during a trip." ;
    rdfs:subClassOf schema:CreativeWork .

# Property Definitions
my:boughtInCity a rdf:Property ;
    rdfs:label "boughtInCity" ;
    rdfs:comment "The city where the souvenir was purchased." ;
    rdfs:domain my:Souvenir ;
    rdfs:range schema:Text .

my:tripYear a rdf:Property ;
    rdfs:label "tripYear" ;
    rdfs:comment "The year the trip took place." ;
    rdfs:domain my:Souvenir ;
    rdfs:range schema:Integer .

my:buyerName a rdf:Property ;
    rdfs:label "buyerName" ;
    rdfs:comment "The name of the person who bought the item." ;
    rdfs:domain my:Souvenir ;
    rdfs:range schema:Text .

my:souvenirCategory a rdf:Property ;
    rdfs:label "souvenirCategory" ;
    rdfs:comment "The type of object, e.g., Keychain, Clothing." ;
    rdfs:domain my:Souvenir ;
    rdfs:range schema:Text .
